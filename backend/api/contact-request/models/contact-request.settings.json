{
  "kind": "collectionType",
  "collectionName": "contact_requests",
  "info": {
    "name": "Contact Request",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "biginteger"
    },
    "message": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "seen",
        "contacted",
        "ignored"
      ]
    }
  }
}
